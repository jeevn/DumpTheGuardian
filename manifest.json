{
	"name": "Dump The Guardian",
	"description": "Divest clicks from the Daily Antisocialist",
	"version": "1.0.1",
	"manifest_version": 2,
	"background" : {
		"scripts" : [
			"background.js"
		]
	},
	"icons": { 
		"16": "img/icon16.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png" 
	},
	"permissions": [
		"webRequest",
		"*://theguardian.com/*",
		"*://www.theguardian.com/*",
		"webRequestBlocking"
	],
  "content_scripts": [{
    "css": ["style.css"],
    "matches": ["*://web.archive.org/*"]
	}]
}